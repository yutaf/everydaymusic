<p class="account"><a href="/account"><%= t 'account.account' %></a></p>
<ul>
  <% @deliveries.each do |delivery| %>
      <li>
        <p class="date_delivered"><%= delivery.date.strftime('%F %T') %></p>
        <div>
          <p class="title"><%= delivery.youtube.title %></p>
          <style>.embed-container { position: relative; padding-bottom: 56.25%; height: 0; overflow: hidden; max-width: 100%; } .embed-container iframe, .embed-container object, .embed-container embed { position: absolute; top: 0; left: 0; width: 100%; height: 100%; }</style><div class='embed-container'><iframe src='https://www.youtube.com/embed/<%= delivery.youtube.videoId %>' frameborder='0' allowfullscreen></iframe></div>
        </div>
      </li>
  <% end %>
</ul>